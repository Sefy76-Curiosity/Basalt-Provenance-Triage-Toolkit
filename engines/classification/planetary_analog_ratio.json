{
  "scheme_name": "Fe/Mn Planetary Analog Ratio",
  "version": "1.1",
  "author": "Sefy Levy",
  "date_created": "2025-02-08",
  "description": "Compare terrestrial basalts to Martian, lunar, or meteoritic analogs using Fe/Mn fingerprint",
  "category": "software",
  "icon": "ü™ê",
  "reference": "Papike et al., 2003; planetary basalt fingerprinting",
  "field": "Planetary Geology",

  "requires_fields": [
    "Fe_ppm",
    "Mn_ppm"
  ],

  "classifications": [
    {
      "name": "MARTIAN ANALOG (Fe/Mn 35-45)",
      "color": "#F44336",
      "description": "Mars-like Fe/Mn ratio - SNC meteorite signature",
      "priority": 1,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": "between",
          "min": 35.0,
          "max": 45.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.85
    },
    {
      "name": "TERRESTRIAL (Fe/Mn 50-70)",
      "color": "#4CAF50",
      "description": "Earth-like Fe/Mn ratio - typical terrestrial basalt",
      "priority": 2,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": "between",
          "min": 50.0,
          "max": 70.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.90
    },
    {
      "name": "LUNAR ANALOG (Fe/Mn 70-90)",
      "color": "#9E9E9E",
      "description": "Moon-like Fe/Mn ratio - lunar basalt signature",
      "priority": 3,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": "between",
          "min": 70.0,
          "max": 90.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.85
    },
    {
      "name": "MARTIAN-TERRESTRIAL TRANSITIONAL",
      "color": "#FF9800",
      "description": "Between Mars and Earth signatures",
      "priority": 4,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": "between",
          "min": 45.0,
          "max": 50.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.70
    },
    {
      "name": "ANOMALOUS / LOW RANGE",
      "color": "#9C27B0",
      "description": "Fe/Mn ratio below known planetary ranges",
      "priority": 5,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": "<",
          "value": 35.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.60
    },
    {
      "name": "ANOMALOUS / HIGH RANGE",
      "color": "#9C27B0",
      "description": "Fe/Mn ratio above known planetary ranges",
      "priority": 6,
      "rules": [
        {
          "field": "Fe_Mn_Ratio",
          "operator": ">",
          "value": 90.0
        }
      ],
      "logic": "AND",
      "confidence_score": 0.60
    },
    {
      "name": "INSUFFICIENT DATA",
      "color": "#607D8B",
      "description": "Missing Fe or Mn data",
      "priority": 999,
      "rules": [],
      "logic": "DEFAULT",
      "confidence_score": 0.0
    }
  ],

  "output_column_name": "Planetary_Analog",
  "add_confidence_column": true,
  "confidence_column_name": "Auto_Confidence",
  "flag_uncertain": true,
  "uncertain_threshold": 0.65,
  "flag_column_name": "Flag_For_Review"
}
