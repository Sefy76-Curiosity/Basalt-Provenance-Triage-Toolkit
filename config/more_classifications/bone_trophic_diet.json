{
  "scheme_name": "Dietary Trophic Classification",
  "version": "1.1",
  "author": "Sefy Levy",
  "description": "Classifies herbivore vs carnivore based on trace element biopurification",
  "category": "bioarchaeology",
  "icon": "ðŸ¥©",

  "requires_fields": [
    "Sr_ppm",
    "Ca_ppm",
    "Ba_ppm"
  ],

  "classifications": [
    {
      "name": "HERBIVORE / PLANT-BASED",
      "color": "green",
      "rules": [
        {
          "field": "Sr_ppm/Ca_ppm",
          "operator": ">",
          "value": 0.002
        }
      ],
      "logic": "AND",
      "confidence_score": 1.0
    },
    {
      "name": "CARNIVORE / MEAT-BASED",
      "color": "saddlebrown",
      "rules": [
        {
          "field": "Sr_ppm/Ca_ppm",
          "operator": "<",
          "value": 0.0008
        }
      ],
      "logic": "AND",
      "confidence_score": 1.0
    }
  ],

  "output_column_name": "Trophic_Level"
}
